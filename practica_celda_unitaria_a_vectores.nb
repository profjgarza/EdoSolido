  RowBox[{
   RowBox[{
    RowBox[{"a", "=", "2."}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"b", "=", "2"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"c", "=", "2."}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"alfa", "=", 
