            RowBox[{"Cos", "[", "alfa", "]"}], "^", "2"}], "\[Minus]", 
           RowBox[{
            RowBox[{"Cos", "[", "beta", "]"}], "^", "2"}], "\[Minus]", 
           RowBox[{
            RowBox[{"Cos", "[", "gama", "]"}], "^", "2"}], "+", 
           RowBox[{"2", " ", 
            RowBox[{"Cos", "[", "alfa", "]"}], " ", 
            RowBox[{"Cos", "[", "beta", "]"}], " ", 
            RowBox[{"Cos", "[", "gama", "]"}]}]}], "]"}]}]}], "}"}]}]}], 
    ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Generando", " ", "los", " ", "v\[EAcute]rtices", " ", "de", " ", "la", 
     " ", "celda"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"vectores", "=", 
     RowBox[{"Graphics3D", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Arrow", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"0", ",", "0", ",", "0"}], "}"}], ",", "vectora"}], "}"}],
          "]"}], ",", 
        RowBox[{"Arrow", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"0", ",", "0", ",", "0"}], "}"}], ",", "vectorb"}], "}"}],
          "]"}], ",", 
        RowBox[{"Arrow", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"0", ",", "0", ",", "0"}], "}"}], ",", "vectorc"}], "}"}],
          "]"}]}], "}"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Generando", " ", "el", " ", "volumen", " ", "que", " ", "envuelve", " ", 
     "a", " ", "los", " ", "v\[EAcute]rtices"}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"volumen", "=", 
     RowBox[{"ConvexHullMesh", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"0", ",", "0", ",", "0"}], "}"}], ",", "vectora", ",", 
        "vectorb", ",", "vectorc", ",", 
        RowBox[{"vectora", "+", "vectorb"}], ",", 
        RowBox[{"vectora", "+", "vectorc"}], ",", 
        RowBox[{"vectorb", "+", "vectorc"}], ",", 
        RowBox[{"vectora", "+", "vectorb", "+", "vectorc"}]}], "}"}], "]"}]}],
     ";"}], "\[IndentingNewLine]", 
   RowBox[{"Show", "[", 
    RowBox[{"vectores", ",", "volumen"}], "]"}]}]}]], "Input",
