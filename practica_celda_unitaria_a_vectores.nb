   RowBox[{"(*", " ", 
    RowBox[{
    "Generaci\[OAcute]n", " ", "de", " ", "los", " ", "vectores", " ", 
     "unitarios"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"vectora", "=", 
     RowBox[{"a", 
      RowBox[{"{", 
       RowBox[{"1", ",", "0", ",", "0"}], "}"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"vectorb", "=", 
     RowBox[{"b", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Cos", "[", "gama", "]"}], ",", 
        RowBox[{"Sin", "[", "gama", "]"}], ",", "0"}], "}"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"vectorc", "=", 
     RowBox[{"c", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Cos", "[", "beta", "]"}], ",", 
        RowBox[{
         RowBox[{
          RowBox[{"Cos", "[", "alfa", "]"}], 
          RowBox[{"Csc", "[", "gama", "]"}]}], "-", 
         RowBox[{
          RowBox[{"Cot", "[", "gama", "]"}], " ", 
          RowBox[{"Cos", "[", "beta", "]"}]}]}], ",", 
        RowBox[{
         RowBox[{"Csc", "[", "gama", "]"}], " ", 
         RowBox[{"Sqrt", "[", " ", 
          RowBox[{"1", "\[Minus]", 
